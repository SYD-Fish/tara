<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.syd.tara.dao.UserDao">
    <select id="get" parameterType="com.syd.tara.bean.UserDO" resultType="com.syd.tara.bean.UserDO">
        select
        id as id,
        name as name ,
        id_card as idCard
        from user where id = #{id}
    </select>
</mapper>